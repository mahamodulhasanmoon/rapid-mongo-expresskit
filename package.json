{
  "name": "rapid-mongo-expresskit",
  "version": "1.5.1",
  "description": "Rapid Mongo Express Kit is a ready-to-use boilerplate for building Express.js applications with MongoDB. It includes pre-configured authentication, mail sending, error handling, and other utilities to streamline development and reduce repetitive tasks.",
  "main": "index.js",
  "bin": {
    "rapid-mongo-expresskit": "./nbin/cli.js",
    "create-module": "./bin/genarator.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint:fix": "npx eslint src --fix",
    "lint": "eslint src --ignore-path .eslintignore --ext .ts",
    "format": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "format:fix": "npx prettier --write src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "create-module": "npx create-module"
  },
  "keywords": [
    "express",
    "mongodb",
    "authentication",
    "boilerplate",
    "redis",
    "development",
    "mail server",
    "error handling",
    "express",
    "mongoose",
    "jwt",
    "bcrypt",
    "nodemailer",
    "cors",
    "cookie-parser",
    "dotenv",
    "ejs",
    "swagger",
    "zod",
    "ioredis"
  ],
  "author": "Mahamodul Hasan Moon",
  "license": "MIT",
  "dependencies": {
    "@types/cookie-parser": "^1.4.9",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.6.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "http-status": "^2.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.2",
    "multer": "2.0.1",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "sharp": "^0.34.2",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.1",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth2": "^0.1.10",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "morgan": "^1.10.0",
    "prettier": "^3.5.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahamodulhasanmoon/rapid-mongo-expresskit.git"
  },
  "bugs": {
    "url": "https://github.com/mahamodulhasanmoon/rapid-mongo-expresskit/issues"
  },
  "homepage": "https://github.com/mahamodulhasanmoon/rapid-mongo-expresskit#readme"
}
